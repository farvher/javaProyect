<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Controlproducto</servlet-name>
        <servlet-class>Controlador.Controlproducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlusuario</servlet-name>
        <servlet-class>Controlador.Controlusuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlventa</servlet-name>
        <servlet-class>Controlador.Controlventa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlsesion</servlet-name>
        <servlet-class>Controlador.Controlsesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>nuevouser</servlet-name>
        <servlet-class>Controlador.nuevouser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controlproducto</servlet-name>
        <url-pattern>/Controlproducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlusuario</servlet-name>
        <url-pattern>/Controlusuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlventa</servlet-name>
        <url-pattern>/Controlventa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlsesion</servlet-name>
        <url-pattern>/Controlsesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nuevouser</servlet-name>
        <url-pattern>/nuevouser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
